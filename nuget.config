<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="local-packages" value="pack/FlyingLogs.Package/artifacts/packages" />
    <add key="nuget-org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="local-packages">
        <package pattern="BekirOzturk.FlyingLogs" />
        <package pattern="BekirOzturk.FlyingLogs.*" />
    </packageSource>
    <packageSource key="nuget-org">
      <package pattern="Microsoft.*" />
      <package pattern="System.*" />
      <package pattern="MSTest.Test*" />
      <package pattern="Crc32.NET" />
      <package pattern="coverlet.collector" />
      <package pattern="SQLitePCLRaw.*" />
      <package pattern="Newtonsoft.Json" />
      <package pattern="StyleCop.Analyzers" />
      <package pattern="runtime.win*" />
      <package pattern="Nuget.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>